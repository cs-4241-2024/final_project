export default function Bracket(props: {name?: string, seed?: string, className?: string}) {
    return (<div className={(props.className ? " " + props.className : "") + "p-4 rounded-3xl bg-sky-50 text-cyan-900 my-2"}>
            <h1>{props.name ? props.name : "Not Determined"}</h1>
            {props.seed ? <h2 className={"text-xs"}>{props.seed}</h2> : ""}
        </div>
    )
}